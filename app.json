{
  "expo": {
    "name": "flappy bird",
    "slug": "flappy-bird-game",
    "description": "Flappy bird that wants to go home",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/flappy-bird.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/flappybird2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
